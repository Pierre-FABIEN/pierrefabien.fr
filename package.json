{
	"name": "sveltekit-boilerplate",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently \"vite dev\" \"npm run prettier:watch\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"prettier:watch": "chokidar \"src/**/*.{js,ts,svelte,scss}\" -c \"npm run prettier:single -- {path}\"",
		"prettier:single": "prettier --write",
		"prettier": "prettier --write \"src/**/*.{js,ts,svelte,scss}\"",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"model-pipeline:run": "node scripts/model-pipeline.js",
		"seed": "node src/lib/server/seed.js"
	},
	"devDependencies": {
		"@internationalized/date": "^3.5.6",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@theatre/studio": "^0.7.2",
		"@types/eslint": "^9.6.0",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^1.0.0-next.45",
		"chokidar": "^4.0.1",
		"chokidar-cli": "^3.0.0",
		"clsx": "^2.1.1",
		"concurrently": "^9.1.0",
		"embla-carousel-svelte": "^8.3.1",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"formsnap": "^2.0.0-next.1",
		"globals": "^15.0.0",
		"lucide-svelte": "^0.454.0",
		"mode-watcher": "^0.4.1",
		"paneforge": "^1.0.0-next.1",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.5",
		"sass-embedded": "^1.80.6",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.20.0",
		"tailwind-merge": "^2.5.4",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.9",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vaul-svelte": "^1.0.0-next.2",
		"vite": "^5.0.3",
		"vite-plugin-mkcert": "^1.17.6",
		"vitest": "^2.0.4",
		"zod": "^3.23.8"
	},
	"dependencies": {
		"@dimforge/rapier3d-compat": "^0.11.2",
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@node-rs/argon2": "^2.0.0",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@oslojs/otp": "^1.0.0",
		"@pilcrowjs/object-parser": "^0.0.4",
		"@prisma/client": "^5.22.0",
		"@theatre/core": "^0.7.2",
		"@threlte/core": "^8.0.0-next.30",
		"@threlte/extras": "^9.0.0-next.39",
		"@threlte/flex": "^2.0.0-next.12",
		"@threlte/rapier": "^2.0.1",
		"@threlte/theatre": "^3.0.0-next.9",
		"@threlte/xr": "^1.0.0-next.13",
		"@types/three": "^0.170.0",
		"apexcharts": "^4.0.0",
		"arctic": "^2.2.2",
		"embla-carousel-auto-scroll": "^8.3.1",
		"embla-carousel-autoplay": "^8.3.1",
		"gsap": "^3.12.5",
		"lucia": "^3.2.2",
		"lucia-auth": "^1.8.0",
		"smooth-scrollbar": "^8.8.4",
		"three": "^0.170.0",
		"uqr": "^0.1.2",
		"vite-plugin-pwa": "^0.21.0"
	}
}
